    "js-yaml": "^3.13.1",
    "node-sass": "^4.12.0",
    "pkginfo": "^0.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-hot-loader": "^4.12.13",
    "sass-loader": "^8.0.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
