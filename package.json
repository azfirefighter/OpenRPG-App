{
  "name": "openrpg",
  "version": "0.5.0",
  "description": "A tool for someone to use to quickly create content for a tabletop RPG world",
  "scripts": {
    "start": "electron-webpack dev",
    "lint": "eslint \"src/**/*\" --fix",
    "prettier": "prettier --write src/**/*.{ts,tsx,js,jsx,md,css,scss,sass,json} !src/static/** !tmp/**",
    "pretty-quick": "pretty-quick --staged",
    "typecheck": "tsc --pretty --noEmit",
    "compile": "electron-webpack",
    "dist": "npm run compile && electron-builder",
    "dist:dir": "npm run compile && electron-builder --dir -c.compression=store -c.mac.identity=null",
    "release": "build --mac --win -p always",
    "postinstall": "electron-rebuild"
  },
  "electronWebpack": {
    "commonSourceDirectory": "src/common",
    "title": "OpenRPG",
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/incomingstick/OpenRPG-App",
    "ssh": "git+ssh://git@github.com:incomingstick/OpenRPG-App.git"
  },
  "license": "OSL",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/incomingstick"
  },
  "author": {
    "name": "Nick Gaulke",
    "url": "https://github.com/incomingstick",
    "email": "incomingstick@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/incomingstick/OpenRPG-App/issues"
  },
  "homepage": "https://openrpg.io",
  "website": {
    "url": "http://openrpg.io",
    "blog": "http://blog.openrpg.io",
    "docs": "http://openrpg.io/documentation/introduction/"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/incomingstick"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "async": ">=3.1.0",
    "electron-compile": ">=6.4.4",
    "electron-fetch": ">=1.3.0",
    "electron-log": "^3.0.9",
    "electron-settings": "^3.2.0",
    "electron-updater": ">=4.1.2",
    "es6-promisify": ">=6.0.2",
    "font-awesome": ">=4.7.0",
    "jquery": ">=3.4.1",
    "lodash": "^4.17.15",
    "mkpath": "^1.0.0",
    "moment": ">=2.24.0",
    "openrpg-libs": "0.0.19",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "semantic-ui-css": ">=2.4.1",
    "semantic-ui-react": ">=0.88.1",
    "source-map-support": "^0.5.16",
    "tslib": ">=1.10.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/async": "^3.0.3",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.146",
    "@types/mkpath": ">=0.1.29",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/eslint-plugin-tslint": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "electron": "^6.1.4",
    "electron-builder": "^22.1.0",
    "electron-prebuilt-compile": ">=4.0.0",
    "electron-rebuild": ">=1.8.6",
    "electron-webpack": ">=2.7.4",
    "electron-webpack-ts": ">=3.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "git-validate": ">=2.2.4",
    "handlebars": "^4.5.1",
    "image-webpack-loader": "^6.0.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.16",
    "sass-loader": ">=8.0.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2"
  }
}
